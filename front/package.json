{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@rehooks/local-storage": "^2.4.0",
    "axios": "^0.19.2",
    "date-fns": "^2.14.0",
    "emotion-theming": "^10.0.27",
    "framer-motion": "^2.0.0-beta.72",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.14.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "semver": "^7.3.2",
    "swr": "^0.2.2"
  },
  "scripts": {
    "start": "PORT=3007 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "typescheck": "tsc --noEmit --jsx react --skipLibCheck",
    "get-types": "swagger-typescript-api -p http://localhost:3000/swagger-json -o ./src/typings -n entities.d.ts --no-client && prettier --write ./src/typings/entities.d.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^25.2.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/semver": "^7.2.0",
    "customize-cra": "^0.9.1",
    "customize-cra-react-refresh": "^1.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-app-rewired": "^2.1.6",
    "swagger-typescript-api": "^1.8.2",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typescheck && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "eslint src/**/*.{ts,tsx,js} --fix",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
